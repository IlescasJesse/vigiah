{
  "compilerOptions": {
    "baseUrl": ".",
    "ignoreDeprecations": "6.0",
    "paths": {
      "@/*": ["./*"],
      "@/components/*": ["./components/*"],
      "@/src/components/*": ["./src/components/*"],
      "@/theme/*": ["./src/theme/*"],
      "@/data/*": ["./src/data/*"],
      "@/app/*": ["./app/*"],
      "@/lib/*": ["./lib/*"],
      "@/models/*": ["./models/*"]
